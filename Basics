# Store variables and convert
num1=int(99.23)
num2=float(23)
num3=str(150)
string1=int("100")
# Print the variables
print(num1)
print(num2)
print(num3)
print(string1)

# Request user full name
user_Name=input("Please provide your full name: ")
# Request user age
user_Age=input("Please provide your age: ")
# Request user house number
user_HseNum=input("Please provide your house number: ")
# Request user street name
user_StNm=input("Please provide the name of your street: ")
# Print all variables
print(f"This is {user_Name}. "
      f"He/She is {user_Age} years old "
      f"and lives at {user_HseNum} {user_StNm}.")
